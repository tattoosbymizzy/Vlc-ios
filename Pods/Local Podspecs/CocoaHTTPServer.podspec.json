{
  "name": "CocoaHTTPServer",
  "version": "2.3",
  "license": "BSD",
  "summary": "A small, lightweight, embeddable HTTP server for Mac OS X or iOS applications.",
  "homepage": "https://github.com/robbiehanson/CocoaHTTPServer",
  "authors": {
    "Robbie Hanson": "cocoahttpserver@googlegroups.com"
  },
  "source": {
    "git": "https://github.com/fkuehne/CocoaHTTPServer.git",
    "tag": "2.3"
  },
  "source_files": "{Core,Extensions,Vendor/CocoaAsyncSocket,Vendor/CocoaLumberjack,Vendor/CocoaLumberjack/Extensions}/**/*.{h,m}",
  "requires_arc": true,
  "platforms": {
    "ios": "5.0",
    "osx": "10.7",
    "tvos": "9.0"
  },
  "ios": {
    "frameworks": [
      "CFNetwork",
      "Security"
    ]
  },
  "tvos": {
    "frameworks": [
      "CFNetwork",
      "Security"
    ]
  },
  "osx": {
    "frameworks": [
      "CoreServices",
      "Security"
    ]
  },
  "libraries": "xml2",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(SDKROOT)/usr/include/libxml2\""
  }
}
